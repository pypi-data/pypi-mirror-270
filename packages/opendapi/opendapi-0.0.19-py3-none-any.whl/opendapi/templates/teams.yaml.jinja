# This is a registry of teams at {{ org_name }}.
# This is used to assign data ownership and create collaboration.
# Add, remove, or modify teams as needed.

schema: https://opendapi.org/spec/0-0-1/teams.json

organization:
  name: {{ org_name }}
  slack_teams:
  # Add slack team ids here
  - T123456

teams:
- urn: {{ org_name_snakecase }}.teams.team_one
  name: TeamOne
  domain: Engineering
  email: grp.teamone@{{ org_email_domain }}
  # Optional fields
  manager_email: manager@{{ org_email_domain }}
  slack_channl_id: C123456
  github_team: org/team-one

- urn: {{ org_name_snakecase }}.teams.team_two
  name: TeamTwo
  domain: Analytics
  email: grp.teamtwo@{{ org_email_domain }}
  # Optional fields
  manager_email: manager@{{ org_email_domain }}
