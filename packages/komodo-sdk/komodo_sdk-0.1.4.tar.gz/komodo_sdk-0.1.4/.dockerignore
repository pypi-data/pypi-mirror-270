# Ignore Git and version control directories
.git
.gitignore

# Ignore build and log files
build/
logs/
*.log
deprecated

# Ignore node modules if using Node.js
node_modules/

# Ignore compiled Python bytecode
__pycache__/
*.pyc
*.pyo
*.pyd

# Ignore virtual environment directories
venv/
.venv/
