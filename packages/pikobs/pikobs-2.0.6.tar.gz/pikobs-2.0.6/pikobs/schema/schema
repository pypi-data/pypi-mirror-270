CREATE TABLE IF NOT EXISTS  header (
  ID_OBS integer,
  LAT real, 
  LON real,
  CODTYP integer, 
  DATE integer, 
  TIME integer,
  ID_STN varchar(50), 
  LOCATION varchar(50), 
  ANTENNA_ALTITUDE REAL, 
  TIME_START INTEGER,
  TIME_END INTEGER,
  RANGE_START REAL ,
  RANGE_END REAL ,
  CENTER_AZIMUTH REAL,
  CENTER_ELEVATION REAL ,
  NOMINAL_PPI_ELEVATION REAL,
  NYQUIST REAL
  );
CREATE TABLE IF NOT EXISTS data (
  ID_DATA integer,
  ID_OBS integer, 
  RANGE REAL,
  VCOORD REAL,
  VARNO integer,
  OBSVALUE real,
  QUALITY_INDEX real,
  VCOORD_TYPE integer,
  FLAG integer,
  FLAG1 integer,
  OMA real,
  OMP real, 
  OBS_ERROR real,
  FG_ERROR real,
  HALF_DELTA_AZIMUTH real,
  HALF_DELTA_RANGE real,
  NUMBER_OBS integer);
