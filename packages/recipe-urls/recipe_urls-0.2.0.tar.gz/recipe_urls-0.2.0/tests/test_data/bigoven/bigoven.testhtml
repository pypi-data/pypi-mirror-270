                if ($.inArray(rid, jsonTrySoons, 0) >= 0) {
                    $('.recipe-tile-full[data-rid="' + rid + '"]')
                        .children()
                        .find('.try-btn-attach')
                        .addClass("try-btn-attach-on");
                    var countText =
                        $('.recipe-tile-full[data-rid="' + rid + '"]').children().find('.upvote-counter').html();
                    countText = "You + " + countText;
                    if (countText === "You + 1") {
                        countText = "You";
                    }
                    $('.recipe-tile-full[data-rid="' + rid + '"]').children().find('.upvote-counter').html(countText);
                }
            });
        }

        if (!bindEventsCompleted) {
            bindEventsCompleted = true;
            bindEvents();
        }
    }

