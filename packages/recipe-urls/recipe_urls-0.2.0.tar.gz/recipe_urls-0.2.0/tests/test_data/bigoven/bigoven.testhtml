    function fillInbox(data, callback) {
        if (data.length === 0) {
            var startingInboxItem = {
                MessageType: "System",
                Actor: { PhotoUrl: "" },
                ActionText: "Welcome! This is your notifications center. For instance, BigOven will let you know if someone's rated a recipe you've posted."
            };

            data = new Array();
            data.push(startingInboxItem);

            if (callback != null) {
                callback('', data.length);
            }
        }

        // var cacheBuster = new Date().getTime();
        var resultHtml = '';

        $.each(
            data,
            function (index, val) {
                var html = buildInboxItem(val);
                resultHtml += html;
            });
        if (callback != null) {
            callback(resultHtml, data.length);
        }
    }

    function updateUserUI(boUser) {
        $(".visible-if-unknown-state").hide();
        if (boUser.UserID > 0) {
            $(".bigoven-friendly-name").html(boUser.UserName);
            $(".visible-if-unauthenticated").hide();
            $(".visible-if-authenticated").show();
            if ((boUser.PremiumExpiryDate !== null)) {
                var dtNow = new Date();
                var expDate = new Date(parseInt(boUser.PremiumExpiryDate.substr(6)));
                if (expDate >= dtNow) {
                    $(".visible-if-not-pro-member").hide();
                    $(".visible-if-pro-member").show();
                    $(".hide-if-pro-member").remove();
                } else {
                    $(".visible-if-pro-member").hide();
                    $(".visible-if-not-pro-member").show();
                }
            } else {
                $(".visible-if-not-pro-member").show();
            }
            if (boUser.UserType > 6) {
                $(".visible-if-admin").show();
                $(".hide-if-admin").hide();
            } else {
                $(".visible-if-admin").hide();
            }
        }
        else {
            $(".visible-if-authenticated").hide();
            $(".visible-if-unauthenticated").show();
            $(".visible-if-not-pro-member").show();
        }
    }

